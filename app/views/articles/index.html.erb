<h1>All Articles</h1>
<%= link_to 'Create New Article', new_article_path %>
<table>
  <tr>
    <th>title</th>
    <th>description</th>
  </tr>
  <% @articles.each do |a| %>
  <tr>
    <td><%= a.title %></td>
    <td><%= a.description %></td>
    <td><%= link_to 'Edit', edit_article_path(a) %></td>
    <td><%= link_to 'Show', article_path(a) %></td>
  </tr>
  <% end %>
</table>